package com.Softcrylic.Exceptions;

public class InvalidPackageNameException
  extends Exception
  implements com.Softcrylic.Intf.Exception
{
  private static final long serialVersionUID = -6256208269283169507L;
  
  public InvalidPackageNameException() {}
  
  public InvalidPackageNameException(String message)
  {
    super(message);
  }
  
  public InvalidPackageNameException(com.Softcrylic.Intf.Exception cause)
  {
    super(cause.getException());
  }
  
  public InvalidPackageNameException(String message, Throwable cause, boolean enableSuppression, boolean writableStackTrace)
  {
    super(message, cause, enableSuppression, writableStackTrace);
  }
  
  public Throwable getException()
  {
    return this;
  }
}
